<h1>Listing auditions - <%= @month.name.upcase %> de <%= @month.year %></h1>

<table>
  <thead>
    <tr>
      <th>Procurador</th>
      <th>Vara</th>
      <th>Turno</th>
      <th>Data</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @auditions.each do |audition| %>
      <tr>
        <td><%= audition.prosecutor_name %></td>
        <td><%= audition.courte %></td>
        <td><%= audition.turn %></td>
        <td><%= l audition.date, format: :short %></td>
        <td><%= link_to 'Show', audition %></td>
        <td><%= link_to 'Destroy', audition, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
